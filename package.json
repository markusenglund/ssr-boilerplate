{
  "name": "ssr-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate project featuring react, ssr, redux-little-router etc",
  "main": "dist/server/index.js",
  "scripts": {
    "build:watch": "webpack --watch",
    "build:prod": "webpack -p",
    "serve": "nodemon dist/server",
    "analyze-bundle": "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json dist/public -s gzip",
    "lint": "eslint --ext .js,.jsx src/",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Markus Englund <markus.s.englund@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-little-router": "^14.2.3",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-react": "^7.0.0-beta.35",
    "@babel/preset-stage-2": "^7.0.0-beta.35",
    "autoprefixer": "^7.2.3",
    "babel-eslint": "^8.1.1",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "ignore-loader": "^0.1.2",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-node-externals": "^1.6.0"
  }
}
